<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.solutions.medadhere.patientregistration">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="Patient Registration"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".MyApplication">

        <activity android:name=".UpdateorNew">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



        <activity android:name=".BeginActivity">
        </activity>

        <activity
            android:name=".QuestionLogActivity"
            android:theme="@style/log">
        </activity>

        <activity android:name=".UpdatePatient">
        </activity>

        <activity android:name=".LifestyleSurvey">
        </activity>
        <activity android:name=".LifeSurvey">
        </activity>
        <activity android:name=".HealthSurvey">
        </activity>

        <activity android:name=".DemogSurvey">
        </activity>

        <activity android:name=".SurveyActivity">
        </activity>

        <activity android:name=".AddMedicationActivity">
        </activity>

        <activity android:name=".DatabaseActivity">
        </activity>
    </application>

</manifest>
